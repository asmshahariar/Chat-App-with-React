{
  "buildCommand": "npm run build",
  "installCommand": "npm install --prefix backend && cd frontend && npm install",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

